<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="android.jbridge.client"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".Shop"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
       
        
        <activity android:label="@string/about_title" android:name=".About"></activity>
        <activity android:label="@string/products_title" android:name=".Products"></activity>
		<activity android:label="@string/products_title" android:name=".Product"></activity>
		<activity android:label="@string/products_title" android:name=".AddProduct"></activity>
		<activity android:label="@string/settings_title" android:name=".Settings"></activity>

    </application>
</manifest>